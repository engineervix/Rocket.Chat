}\n\n    LoginPresence.started = true;\n  }\n\n};\nLoginPresence.start();","map":{"version":3,"sources":["app/lib/client/lib/LoginPresence.js"],"names":["module","export","LoginPresence","Meteor","link","v","settings","awayTime","started","startTimer","stopTimer","timer","setTimeout","disconnect","clearTimeout","status","userId","get","connect","reconnect","start","window","addEventListener","document","hasFocus"],"mappings":"AAAAA,MAAM,CAACC,MAAP,CAAc;AAACC,EAAAA,aAAa,EAAC,MAAIA;AAAnB,CAAd;AAAiD,IAAIC,MAAJ;AAAWH,MAAM,CAACI,IAAP,CAAY,eAAZ,EAA4B;AAACD,EAAAA,MAAM,CAACE,CAAD,EAAG;AAACF,IAAAA,MAAM,GAACE,CAAP;AAAS;;AAApB,CAA5B,EAAkD,CAAlD;AAAqD,IAAIC,QAAJ;AAAaN,MAAM,CAACI,IAAP,CAAY,mBAAZ,EAAgC;AAACE,EAAAA,QAAQ,CAACD,CAAD,EAAG;AAACC,IAAAA,QAAQ,GAACD,CAAT;AAAW;;AAAxB,CAAhC,EAA0D,CAA1D;AAIvH,MAAMH,aAAa,GAAG;AAC5BK,EAAAA,QAAQ,EAAE,MADkB;AACV;AAClBC,EAAAA,OAAO,EAAE,KAFmB;;AAI5BC,EAAAA,UAAU,GAAG;AACZP,IAAAA,aAAa,CAACQ,SAAd;;AACA,QAAI,CAACR,aAAa,CAACK,QAAnB,EAA6B;AAC5B;AACA;;AACD,SAAKI,KAAL,GAAaC,UAAU,CAACV,aAAa,CAACW,UAAf,EAA2BX,aAAa,CAACK,QAAzC,CAAvB;AACA,GAV2B;;AAW5BG,EAAAA,SAAS,GAAG;AACXI,IAAAA,YAAY,CAAC,KAAKH,KAAN,CAAZ;AACA,GAb2B;;AAc5BE,EAAAA,UAAU,GAAG;AACZ,UAAME,MAAM,GAAGZ,MAAM,CAACY,MAAP,EAAf;;AACA,QAAIA,MAAM,IAAIA,MAAM,CAACA,MAAP,KAAkB,SAAhC,EAA2C;AAC1C,UAAI,CAACZ,MAAM,CAACa,MAAP,EAAD,IAAoBV,QAAQ,CAACW,GAAT,CAAa,6BAAb,MAAgD,IAAxE,EAA8E;AAC7Ed,QAAAA,MAAM,CAACU,UAAP;AACA;AACD;;AACDX,IAAAA,aAAa,CAACQ,SAAd;AACA,GAtB2B;;AAuB5BQ,EAAAA,OAAO,GAAG;AACT,UAAMH,MAAM,GAAGZ,MAAM,CAACY,MAAP,EAAf;;AACA,QAAIA,MAAM,IAAIA,MAAM,CAACA,MAAP,KAAkB,SAAhC,EAA2C;AAC1CZ,MAAAA,MAAM,CAACgB,SAAP;AACA;AACD,GA5B2B;;AA6B5BC,EAAAA,KAAK,GAAG;AACP,QAAIlB,aAAa,CAACM,OAAlB,EAA2B;AAC1B;AACA;;AAEDa,IAAAA,MAAM,CAACC,gBAAP,CAAwB,OAAxB,EAAiC,MAAM;AACtCpB,MAAAA,aAAa,CAACQ,SAAd;AACAR,MAAAA,aAAa,CAACgB,OAAd;AACA,KAHD;AAKAG,IAAAA,MAAM,CAACC,gBAAP,CAAwB,MAAxB,EAAgC,MAAM;AACrCpB,MAAAA,aAAa,CAACO,UAAd;AACA,KAFD;;AAIA,QAAI,CAACY,MAAM,CAACE,QAAP,CAAgBC,QAAhB,EAAL,EAAiC;AAChCtB,MAAAA,aAAa,CAACO,UAAd;AACA;;AAEDP,IAAAA,aAA